    "replace-bundle-webpack-plugin": "^1.0.0",
    "script-ext-html-webpack-plugin": "^2.1.4",
    "sinon": "^7.5.0",
    "sinon-chai": "^3.3.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.0.0",
    "url-loader": "^2.2.0",
    "v8-lazy-parse-webpack-plugin": "^0.3.0",
    "webpack": "^4.41.1",
    "webpack-dashboard": "^3.2.0",
    "webpack-dev-server": "^3.8.2"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "preact": "^10.0.0",
    "preact-compat": "^3.0.0",
